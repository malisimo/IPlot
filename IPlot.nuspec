<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
    <metadata>
        <id>IPlot</id>
        <title>IPlot</title>
        <version>0.0.1-pre2</version>
        <description>Charting library for generating charts with HighCharts or Plotly</description>
        <authors>Matt Jones</authors>
        <license type="expression">MIT</license>
        <requireLicenseAcceptance>false</requireLicenseAcceptance>
        <tags>iplot plot chart interactive highcharts plotly</tags>
        <projectUrl>https://github.com/malisimo/IPlot</projectUrl>        
        <repository type="git" url="https://github.com/malisimo/IPlot.git" branch="master" />
        <dependencies>
            <group targetFramework="netstandard2.0">
                <dependency id="FSharp.Core" version="4.7.2" />
                <dependency id="Microsoft.CSharp" version="4.7.0" />
                <dependency id="Newtonsoft.Json" version="12.0.3" />
            </group>
        </dependencies>
        <icon>Icon.png</icon>
    </metadata>
    <files>
        <file src="src\IPlot\bin\Release\netstandard2.0\*.*" target="lib\netstandard2.0" />
        <file src="Icon.png" target="" />
    </files>
</package>
